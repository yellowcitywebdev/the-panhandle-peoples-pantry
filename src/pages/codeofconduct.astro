---
import Container from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import SectionHead from "../components/PageBanner.astro";
import CodeOfConductComponent from "../components/CodeOfConduct.astro";

import CtaBanner from "../components/CtaBanner.astro";

const title = "Code of Conduct";
const desc = "";
const image = "/hero-inner.webp";
---

<BaseLayout
  title="Code of Conduct"
  description="The Panhandle People's Pantry Code of Conduct"
>
  <SectionHead title={title} desc={desc} image={image} />
  <Container>
    <CodeOfConductComponent />
  </Container>
  <CtaBanner />
</BaseLayout>
