---
import Container from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import SectionHead from "../components/PageBanner.astro";
import BylawsComponent from "../components/Bylaws.astro";
import CtaBanner from "../components/CtaBanner.astro";

const title = "Bylaws";
const desc = "";
const image = "/hero-inner.webp";
---

<BaseLayout title="Bylaws" description="The Panhandle People's Pantry Bylaws">
  <SectionHead title={title} desc={desc} image={image} />
  <Container>
    <BylawsComponent />
  </Container>
  <CtaBanner />
</BaseLayout>
